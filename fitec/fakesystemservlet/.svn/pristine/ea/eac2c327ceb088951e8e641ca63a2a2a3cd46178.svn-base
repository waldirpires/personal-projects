<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app id="WebApp_ID">
	<display-name>FakeSystemServlet</display-name>
  <description>Apache SOAP ServletClient</description>
  <servlet>
    <servlet-name>RTVServlet</servlet-name>
    <display-name>RTVServlet</display-name>
    <description>Servlet that returns SOAP messages to RighTV Adapter</description>
    <servlet-class>com.ericsson.servlet.fakesystem.RTVServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CDNServlet</servlet-name>
    <display-name>CDNServlet</display-name>
    <description>Servlet that returns SOAP messages to CDN Adapter</description>
    <servlet-class>com.ericsson.servlet.fakesystem.CDNServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RTVServlet</servlet-name>
    <url-pattern>/RTVServlet</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>CDNServlet</servlet-name>
    <url-pattern>/CDNServlet</url-pattern>
  </servlet-mapping>
</web-app>
